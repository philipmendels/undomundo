{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --no-cache",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "undomundo",
  "author": "Philip Mendels",
  "module": "dist/undomundo.esm.js",
  "size-limit": [
    {
      "path": "dist/undomundo.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/undomundo.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "ts-unused-exports": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "fp-ts": "^2.10.4",
    "fp-ts-std": "^0.10.0"
  }
}
